{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Dit.UseAzureBlobStorage": false,
  "ConnectionStrings": {
    "umbracoDbDSN": ""
  },
  "Umbraco": {
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "",
          "ContainerName": ""
        }
      }
    },
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "RuntimeMinification": {
        "UseInMemoryCache": false,
        "CacheBuster": "AppDomain"
      },
      "Global": {
        "Id": "6156d136-1d32-4b77-8d18-2b566ef5d3f4"
      }
    }
  }
}
